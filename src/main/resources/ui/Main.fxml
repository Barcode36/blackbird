            <StackPane fx:id="menuPane" prefWidth="270.0" />
            <StackPane fx:id="outerStackPane" HBox.hgrow="ALWAYS">
               <children>
                  <ScrollPane fx:id="scrollPane" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" pannable="true" prefHeight="800.0" prefWidth="1010.0" styleClass="edge-to-edge">
